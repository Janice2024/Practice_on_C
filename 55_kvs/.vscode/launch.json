{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug",
            "type": "cppdbg",
            "request": "launch",
            // This is the path for the program you want to debug. Make sure
            // "cwd" points to the folder you want the program to be run in.
            "program": "${workspaceFolder}/debug-example/sumfactors-test",
            "cwd": "${workspaceFolder}/debug-example",
            // If you need give arguments when starting the programming put them
            // below. Example is given.
            // "args": [
            //     "arg1",
            //     "arg2",
            //     "arg3"
            // ],
            "args": [],
            "stopAtEntry": true,
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
